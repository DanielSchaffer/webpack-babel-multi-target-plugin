    "copy-webpack-plugin": "^5.0.0",
    "mini-css-extract-plugin": "^0.4.3",
    "aurelia-webpack-plugin": "^3.0.0",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "expose-loader": "^0.7.5",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
